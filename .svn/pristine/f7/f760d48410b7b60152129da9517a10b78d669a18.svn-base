<!DOCTYPE html>
<html>
<meta charset="utf-8">
<head th:include="include :: header"></head>
<body class="gray-bg">
	<div class="wrapper wrapper-content ">
		<div class="row">
			<div class="col-sm-12">
				<div class="ibox float-e-margins">
					<div class="ibox-content">
						<form class="form-horizontal m-t" id="signupForm">
						<input id="id" name="id" th:value="${onlineSensation.id}"  type="hidden">
							<div class="form-group">	
								<label class="col-sm-3 control-label">网红公司id：</label>
								<div class="col-sm-8">
									<input id="whid" name="whid" th:value="${onlineSensation.whid}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">网红公司名称：</label>
								<div class="col-sm-8">
									<input id="whname" name="whname" th:value="${onlineSensation.whname}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">联系方式：</label>
								<div class="col-sm-8">
									<input id="whphone" name="whphone" th:value="${onlineSensation.whphone}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">联系地址：</label>
								<div class="col-sm-8">
									<input id="whaddress" name="whaddress" th:value="${onlineSensation.whaddress}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">联系邮箱：</label>
								<div class="col-sm-8">
									<input id="whemail" name="whemail" th:value="${onlineSensation.whemail}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">网红人数：</label>
								<div class="col-sm-8">
									<input id="whnums" name="whnums" th:value="${onlineSensation.whnums}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">收益总额：</label>
								<div class="col-sm-8">
									<input id="earningsTotal" name="earningsTotal" th:value="${onlineSensation.earningsTotal}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">提现总额：</label>
								<div class="col-sm-8">
									<input id="withdrawTotal" name="withdrawTotal" th:value="${onlineSensation.withdrawTotal}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">创建时间：</label>
								<div class="col-sm-8">
									<input id="createTime" name="createTime" th:value="${onlineSensation.createTime}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">最新修改时间：</label>
								<div class="col-sm-8">
									<input id="updateTime" name="updateTime" th:value="${onlineSensation.updateTime}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">状态 1：正常 2：停止：</label>
								<div class="col-sm-8">
									<input id="status" name="status" th:value="${onlineSensation.status}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">备注：</label>
								<div class="col-sm-8">
									<input id="remark" name="remark" th:value="${onlineSensation.remark}" class="form-control" type="text">
								</div>
							</div>
																					<div class="form-group">
								<div class="col-sm-8 col-sm-offset-3">
									<button type="submit" class="btn btn-primary">提交</button>
								</div>
							</div>
						</form>
					</div>
				</div>
			</div>
	</div>
	</div>
	<div th:include="include::footer"></div>
	<script type="text/javascript" src="/js/appjs/sensation/onlineSensation/edit.js">
	</script>
</body>
</html>
